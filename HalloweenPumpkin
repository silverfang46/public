#In C
#include <stdio.h>
#include <unistd.h>

#define P(x) printf("\033[38;5;208m%s\033[0m", x)
#define S 150000

char *o[] = {
    "            Happy Halloween!\n"
    "                 //\n       _.-\"\"\"\"\"'//-'\"\"\"\"\"-._\n     .', ,  , , : : ` ` `  `.\n    / , , \\'.-_ : :_.-'/ ` ` \\\n   / , ,  :\\(_)/  /(_)/ : ` ` \\\n  | , ,  ,  \\__//\\\\__/ . . ` ` |\n  | . .:_  : : '--`: : . _: ; :|\n  | : : \\\\_  _' : _: :__// , , |\n   \\ ` ` \\ \\/ \\/\\/ \\_/  / , , /\n    \\ ` ` \\_/\\_/\\_/\\_/\\/ , , /\n     `._ ` . :  :  :  , , _.'\n        `-..............-'  ",
    "            Happy Halloween!\n"
    "                 //\n       _.-\"\"\"\"\"'//-'\"\"\"\"\"-._\n     .' ,   ,   : : ` ` `   `.\n    /  ,    \\'.-._: :_.-'` `  \\\n   /   ,   :\\(_)(/ /(_)/ ` `  \\\n  |    ,    \\__//\\\\__/  .  `  |\n  |  ,.:_   : : '--`-.: ._:;: |\n  | : : \\\\_   _:.: _:_::__,  ,|\n   \\ `  ` \\   \\/ \\/\\/\\_/  ,  /\n    \\ ` `  \\_/\\_/\\_/\\_/\\, ,  /\n     `._ `  :  :  :   ` _.' \n        `-.............-'     ",

    "            Happy Halloween!\n"
    "                 //\n       _.-\"\"\"\"\"'//-'\"\"\"\"\"-._\n     .'         : :        `.\n    /           : :           \\\n   /            (_)            \\\n  |             //             |\n  |            //             |\n  |          __//__           |\n   \\         \\____/          /\n    \\                      /\n     `._                _.'\n        `-............-'    ",

    "            Happy Halloween!\n"
    "                 //\n       _.-\"\"\"\"\"'//-'\"\"\"\"\"-._\n     .' ,   ,   : : ` ` `   `.\n    /  ,    \\'.-._: :_.-'` `  \\\n   /   ,   :\\(_)(/ /(_)/ ` `  \\\n  |    ,    \\__//\\\\__/  .  `  |\n  |  ,.:_   : : '--`-.: ._:;: |\n  | : : \\\\_   _:.: _:_::__,  ,|\n   \\ `  ` \\   \\/ \\/\\/\\_/  ,  /\n    \\ ` `  \\_/\\_/\\_/\\_/\\, ,  /\n     `._ `  :  :  :   ` _.' \n        `-.............-'     "
};

void g(int a) {
    int p, q;
    for (p = 0; p < a; p++) {
        for (q = 0; q < 4; q++) {
            printf("\033[H\033[J");
            P(o[q]); 
            usleep(S);
        }
    }
}

int main() {
    g(15);
    return 0;
}
